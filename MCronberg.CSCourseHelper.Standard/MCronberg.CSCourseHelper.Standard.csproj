<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Michell Cronberg</Authors>
    <Description>Simple package containg types used i C# training. See project site for examples.</Description>
    <Copyright>Michell Cronberg</Copyright>
    <PackageProjectUrl>https://github.com/devcronberg/MCronberg.CSCourseHelper.Standard</PackageProjectUrl>
    <RepositoryUrl>https://github.com/devcronberg/MCronberg.CSCourseHelper.Standard.git</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.10</Version>
    <PackageReleaseNotes>Autogenerated backing fields/methods</PackageReleaseNotes>
    <FileVersion>1.0.10.0</FileVersion>
    <AssemblyVersion>1.0.10.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.1" />
  </ItemGroup>
</Project>